<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/aapt"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <size
        android:layout_weight="1"
        android1:width="1mm"
        android1:height="10dp"
        />
    <gradient
        android1:startColor="#fdfbfb"
        android1:endColor="#faf9f9"
        android1:angle="270"
        />

    <corners
        android1:topLeftRadius="30dp"
        android1:topRightRadius="30dp"
        />
    <padding
        android1:left="1dp"
        android1:right="1dp"
        android1:top="1dp"
        android1:bottom="1dp"
        />

</shape>
