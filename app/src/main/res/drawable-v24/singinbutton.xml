        android1:topLeftRadius="10dp"
        android1:topRightRadius="10dp"
        android1:bottomRightRadius="10dp"
        android1:bottomLeftRadius="10dp"
        />
    <padding
        android1:left="1dp"
        android1:right="1dp"
        android1:top="1dp"
        android1:bottom="1dp"
        />

</shape>
